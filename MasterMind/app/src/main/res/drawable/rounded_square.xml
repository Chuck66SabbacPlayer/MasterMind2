<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="ring"
            android:innerRadiusRatio="1.6"
            android:useLevel="false"
            android:thickness="1000dp">
            <solid
                android:color="@color/design_default_color_background"
                />

        </shape>
    </item>

    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">

            <size
                android:width="50dp"
                android:height="50dp"
                />
            <solid />
            <stroke
                android:color="@color/black"
                android:width="3dp"
                />

            <corners
                android:radius="5dp"
                />
        </shape>

    </item>
    <item
        android:height="50dp"
        android:width="50dp"
        android:gravity="center">
        <shape
            android:shape="rectangle">
            <solid

                />
            <stroke
                android:color="@color/design_default_color_background"
                android:width="1dp" />

            <corners
                android:radius="5dp"
                />

            <size
                android:width="50dp"
                android:height="50dp"
                />


        </shape>
    </item>



</layer-list>

